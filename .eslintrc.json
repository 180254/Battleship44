{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "strict": ["error", "global"],
    "no-unused-vars": ["warn", {"vars": "all", "args": "all"}],
    "quotes": ["error", "double", {"avoidEscape": true, "allowTemplateLiterals": true}],
    "eqeqeq": ["error"],
    "func-style": [
      "error",
      "expression"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
